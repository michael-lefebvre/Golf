{
  "name": "Squid",
  "description": "Squid gives you a quick access to all your Github repositories.",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/michael-lefebvre/Squid",
  "bugs": "https://github.com/michael-lefebvre/Squid/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/michael-lefebvre/Squid"
  },
  "keywords": [
    "react",
    "github",
    "node-webkit",
    "nw.js"
  ],
  "main": "index.html",
  "serviceUrl": "https://api.github.com/",
  "repoPagination": 30,
  "single-instance": true,
  "debug": true,
  "window": {
    "title": "Squid",
    "toolbar": false,
    "width": 380,
    "height": 465,
    "resizable": false,
    "frame": false,
    "transparent": true,
    "show": false
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "pubsub-js": "^1.5.2",
    "react": "^0.13.1",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "browserify": "^7.0.3",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-concat": "^2.2.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^0.7.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-watch": "^0.6.4",
    "node-bourbon": "1.2.3",
    "reactify": "^0.17.1",
    "vinyl-source-stream": "^1.0.0"
  }
}
